/* pages/car_list/car_list.wxss */
@import "../../css/public.wxss";

.address-add {
  float: right;
  font-size: 0.8em;
  padding-right: 10px;
  margin-top: -50px;
}

.content {
  width: 100%;
  background: #fff;
  padding: 3%;
  margin-bottom: 30px;
  height: 120px;
}

.font-css {
  font-size: 0.8em;
  color: #666;
  height: 35px;
  line-height: 35px;
  border-bottom: 2px solid #f5f5f5;
  width: 94%;
}

.button-css {
  width: 70px;
  height: 30px;
  background: #fff;
  color: #666;
  font-size: 0.8em;
  float: right;
  margin: 10px 0;
  line-height: 30px;
  margin-right: 6%;
}

.button-hide {
  display: none;
}
